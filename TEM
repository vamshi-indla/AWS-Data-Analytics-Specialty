select 
	names.name_first as first_name,
    names.name_last as last_name,
    location.location_state as state,
    age.dob_age as age, 
    contact.cell as cell,
    picture.picture_large as picture
 from names
 	join location on names.id_value = location.id_value
    join age on age.id_value = names.id_value
    join contact on names.id_value = contact.id_value
    join picture on names.id_value = picture.id_value
order by age
limit 10;